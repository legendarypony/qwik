{
  "name": "qwik-site",
  "version": "0.0.0",
  "description": "Qwik Site.",
  "scripts": {
    "build": "yarn build.qwest && yarn build.client && yarn build.server",
    "build.client": "vite build",
    "build.server": "vite build --outDir server/build --ssr src/entry.server.tsx",
    "build.qwest": "cd qwest && yarn build",
    "preview": "vite preview",
    "dev": "yarn dev.ssr",
    "dev.ssr": "vite",
    "dev.client": "vite --mode clientonly",
    "dev.wrangler": "wrangler pages dev ./dist",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "start": "yarn dev.client",
    "link.builder": "npm link @builder.io/qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.5.2",
    "@builder.io/qwik": "0.0.18-7-dev20220405193739",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@cloudflare/workers-types": "^3.2.0",
    "@mdx-js/mdx": "^2.0.0",
    "@microsoft/api-extractor": "^7.19.5",
    "@types/github-slugger": "^1.3.0",
    "@types/marked": "^4.0.2",
    "@types/node": "^17.0.21",
    "@types/refractor": "^3.0.2",
    "@types/service-worker-mock": "2.0.1",
    "autoprefixer": "10.4.2",
    "estree-util-value-to-estree": "^1.3.0",
    "front-matter": "^4.0.2",
    "github-slugger": "^1.4.0",
    "hast-util-heading-rank": "^2.1.0",
    "hast-util-to-string": "^2.0.0",
    "history": "^5.3.0",
    "marked": "^4.0.12",
    "postcss": "8.4.6",
    "prettier": "2.3.0",
    "prism-themes": "^1.9.0",
    "refractor": "^4.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "slugify": "^1.6.5",
    "source-map": "^0.7.3",
    "tailwindcss": "3.0.18",
    "tsm": "^2.2.1",
    "typescript": "^4.5.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0",
    "uvu": "^0.5.3",
    "vfile": "^5.3.2",
    "vite": "^2.7.13",
    "wrangler": "beta"
  },
  "author": "Builder.io Team",
  "engines": {
    "node": ">=14"
  },
  "license": "MIT"
}
