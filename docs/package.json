  "main": "src/main.tsx",
  "scripts": {
    "build": "yarn build.qwest && yarn build.client && yarn build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --mode ssr",
    "build.qwest": "cd qwest && yarn build",
