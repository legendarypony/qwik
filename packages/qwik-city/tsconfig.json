{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["es2020", "DOM"],
    "strict": true,
    "importsNotUsedAsValues": "error",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@builder.io/qwik-city": ["src/runtime/index"],
      "@builder.io/qwik-city/build": ["src/runtime/build"]
    },
    "types": ["vite/client"],
    "outDir": "tsc-out"
  },
  "include": ["src"]
}
