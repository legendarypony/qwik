    "concurrently": "^6.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.2",
    "wait-on": "^6.0.0"
  },
